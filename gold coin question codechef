#include <stdio.h>

int main(void) {
    int A, B, X, Y;
    scanf("%d %d %d %d", &A, &B, &X, &Y);
    
    if(X > Y) {
        printf("%d\n", A);  // Chef wins, so print A (winner's coins)
    } else {
        printf("%d\n", B);  // Chef loses, so print B (loser's coins)
    }
    
    return 0;
}
